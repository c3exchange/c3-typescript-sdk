{
    "compilerOptions": {
      "types": [ "node", "jest" ],
      "lib": [ "es2021" ],
      "module": "commonjs",
      "target": "es2021",
      "declaration": true,
      "declarationMap": true,
      "esModuleInterop": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "strict": true,
      "sourceMap": true,
      "forceConsistentCasingInFileNames": true,
      "noUnusedLocals": false,
      "noEmitOnError": true
    }
  }
  